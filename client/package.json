      "plugin:vue/strongly-recommended",
      "plugin:prettier/recommended",
      "@vue/prettier",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": 0,
      "vue/no-v-model-argument": 0
