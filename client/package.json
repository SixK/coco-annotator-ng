    "nopt": "^7.2.0",
    "paper": "^0.12.17",
    "pinia": "^2.1.7",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "postcss-modules": "^6.0.0",
    "simplify-js": "^1.2.4",
    "@unlazy/vue": "^0.11.1",
    "vue": "^3.4.21",
    "vue-3-socket.io-ext": "^1.2.1",
    "vue-loading-overlay": "^6.0.4",
